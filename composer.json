{
    "name": "robinmoisson/select2-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 bundle that integrates Select2 as a drop-in replacement for a standard entity field on a Symfony form.",
    "keywords": ["symfony","select2", "typeahead","autocomplete"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Keatinge",
            "email": "tetranz@gmail.com"
        },
        {
            "name": "Robin Moisson",
            "email": "dev@robinmoisson.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": ">=2.2",
        "doctrine/orm": ">=2.4"
    },
    "autoload": {
        "psr-0": { "Tetranz\\Select2Bundle": "" }
    },
    "target-dir": "Tetranz/Select2Bundle",
    "extra": {
        "branch-alias": {
        }
    }
}
